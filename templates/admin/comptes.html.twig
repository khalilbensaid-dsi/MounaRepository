<h3 style="color:#10aeda">La liste des sondés :</h3>
{% for sonde in sondes %}
    <table class="table">
    <tr>
    <th>cin</th>
    <th>nom</th>
    <th>prénom</th>
    <th>age</th>
    <th>date de Naissance</th>
    <th>email</th>
    <th>tel</th>
    <th>genre</th>
    <th>adresse</th>
    </tr>
    <tr>
    <td>{{sonde.cin}}</td>
    <td>{{sonde.nom}}</td>
    <td>{{sonde.prenom}}</td>
    <td>{{sonde.age}}</td>
    <td>{{sonde.dateNaissance|date('d/m/Y')}}</td>
    <td>{{sonde.email}}</td>
    <td>{{sonde.tel}}</td>
    <td>
    {% if sonde.genre == 1 %}
    homme
    {% else %}
        femme
    {% endif %}
    </td>
    <td>{{sonde.adresse}}</td>
    </tr>
    </table>
{% endfor %}
<h3 style="color:#10aeda">La liste des enqueteurs </h3>
