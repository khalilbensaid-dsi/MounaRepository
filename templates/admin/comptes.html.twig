        <td>{{enqueteur.prenom}}</td>
        <td>{{enqueteur.age}}</td>
        <td>{{enqueteur.dateNaissance|date('d/m/Y')}}</td>
        <td>{{enqueteur.email}}</td>
        <td>{{enqueteur.tel}}</td>
        <td>{% if enqueteur.genre == 1 %}
    homme
    {% else %}
        femme
    {% endif %}</td>
        <td>{{enqueteur.matriculeFiscale}}</td>
        <td>{{enqueteur.adresse}}</td>
    </tr>
</table>
{% endfor %}
